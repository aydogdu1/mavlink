	<messages>

		<message id="7002" name="STOP_MSG">
			<description>tüm gorevleri durdurmak icin  offboard mesaji</description>
			<field type="uint64_t" name="timestamp"></field>
		</message>


		<message id="7003" name="DALIS_MSG">
			<description>dalış için yukarıya gönderilecek mesaj</description>
			<field type="uint64_t" name="timestamp"></field>
			<field type="float" name="yukseklik"></field>
			<field type="float" name="gelis_acisi"></field>
			<field type="float" name="dalis_acisi"></field>
			<field type="float" name="alt_sinir"></field>
			<field type="float" name="loiter_radius"></field>

		</message>

		<message id="7004" name="NOKTAYA_GIT_MSG">
			<description>noktaya gitmek için mesaj</description>
			<field type="uint64_t" name="timestamp"></field>
			<field type="float" name="x_m"></field>
			<field type="float" name="y_m"></field>
		</message>


		<message id="7005" name="KILIT_MSG">
			<description>rakip gps  bilgileri</description>
			<field type="uint64_t" name="timestamp"></field>
			<field type="float" name="latitude"></field>
			<field type="float" name="longitude"></field>
			<field type="float" name="yukseklik"></field>
			<field type="float" name="hiz"></field>
			<field type="float" name="heading"></field>
			<field type="float" name="roll"></field>
			<field type="float" name="pitch"></field>
		</message>

		<message id="7006" name="QR_SET_MSG">
