			<field type="uint64_t" name="timestamp"></field>
			<field type="float" name="latitude"></field>
			<field type="float" name="longitude"></field>
		</message>


	
		<message id="2002" name="H1_KILIT_MSG">
			<description>sunucuya gönderilecek kilit bilgileri</description>
			<field type="uint64_t" name="timestamp"></field>
			<field type="uint16_t" name="baslangic_saat"></field>
			<field type="uint16_t" name="baslangic_dakika"></field>
			<field type="uint16_t" name="baslangic_saniye"></field>
			<field type="uint16_t" name="baslangic_milisaniye"></field>
