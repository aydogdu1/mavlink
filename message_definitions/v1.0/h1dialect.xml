			<field type="uint16_t" name="dakika"></field>
			<field type="uint16_t" name="saniye"></field>
			<field type="uint16_t" name="milisaniye"></field>

		</message>


		<message id="2007" name="H1_DALIS_MSG">
			<description>sunucuya gönderilecek dalış bilgileri</description>
