			<field type="uint16_t" name="bitis_saniye"></field>
			<field type="uint16_t" name="bitis_milisaniye"></field>
		</message>


		<message id="2004" name="H1_PID_KILIT">
			<description>kilit  pıd katsayıları</description>
			<field type="uint64_t" name="timestamp"></field>
			<field type="float" name="roll_p"></field>
			<field type="float" name="roll_i"></field>
			<field type="float" name="roll_d"></field>
			<field type="float" name="pitch_p"></field>
			<field type="float" name="pitch_i"></field>
			<field type="float" name="pitch_d"></field>
			<field type="float" name="throttle_p"></field>
			<field type="float" name="throttle_i"></field>
			<field type="float" name="throttle_d"></field>

		</message>

		<message id="2005" name="H1_PID_YAKLASMA">
			<description>yaklaşma pıd katsayıları</description>
			<field type="uint64_t" name="timestamp"></field>
			<field type="float" name="hiz_p"></field>
			<field type="float" name="hiz_i"></field>
			<field type="float" name="hiz_d"></field>

		</message>

		<message id="2006" name="H1_SUNUCU_SAATI">
			<description>sunucuya gönderilecek sunucu saati bilgileri</description>
			<field type="uint64_t" name="timestamp"></field>
			<field type="uint16_t" name="gUn"></field>
